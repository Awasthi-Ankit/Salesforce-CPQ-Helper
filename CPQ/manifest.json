{
    "manifest_version": 3,
    "name": "CPQ",
    "version": "1.0",
    "description": "CPQ Helper",
    "permissions": ["scripting","tabs","cookies"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },"icons": {
      "16": "/img/icon16.png",
      "48": "/img/icon48.png",
      "128": "/img/icon128.png"
    }
  }
  